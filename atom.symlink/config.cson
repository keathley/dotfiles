"*":
  Zen:
    fullscreen: false
    showWordCount: "Left"
    tabs: "multiple"
  achievements: {}
  "atom-beautify":
    _analyticsUserId: "b8290516-d866-4239-91c8-c37c8d6972da"
  "atom-material-ui":
    altCmdPalette: true
    depth: true
    disableAnimations: true
    panelContrast: true
    slimScrollbar: true
  "autocomplete-elixir":
    elixirPath: "/usr/local/bin/elixir"
    erlangHome: "/usr/local/bin"
  "autocomplete-plus": {}
  "base16-syntax":
    scheme: "Flat"
  "circle-ci": {}
  core:
    audioBeep: false
    disabledPackages: [
      "achievements"
      "incremental-search"
      "minimap"
      "linter-jsxhint"
      "linter-less"
      "linter-eslint"
    ]
    packagesWithKeymapsDisabled: [
      "dash"
    ]
    themes: [
      "one-dark-ui"
      "seti-syntax"
    ]
  editor:
    fontFamily: "\"FiraCode-Retina\""
    fontSize: 15
    invisibles: {}
    showIndentGuide: true
  "exception-reporting":
    userId: "e61f6830-23ef-e75c-9b6a-0332363e4385"
  "find-and-replace":
    scrollToResultOnLiveSearch: true
  "flex-tool-bar": {}
  fonts:
    fontFamily: "FiraCode"
  "fuzzy-finder": {}
  "git-log": {}
  "go-plus":
    goPath: "~/Development/Gocode"
    lintOnSave: false
  "haskell-ghc-mod":
    onChangeLint: true
  "ide-haskell": {}
  "ide-haskell-repl": {}
  "imdone-atom":
    openIn: {}
    todaysJournal:
      directory: "/Users/keathley/Dropbox/Journal"
  "isotope-ui":
    fontFamily: "Roboto"
    minimalMode: true
  "language-babel":
    transpileOnSave: false
  "latex-completions": {}
  "less-than-slash": {}
  linter:
    errorPanelHeight: 26
    lintOnChange: false
    lintOnEditorFocus: false
    lintOnFly: false
    showErrorInline: false
  "linter-eslint":
    eslintRulesDir: "~/"
    globalNodePath: "/usr/local"
    useGlobalEslint: true
  "linter-jshint": {}
  "linter-jsxhint":
    harmony: true
  "linter-rubocop": {}
  "linter-rust": {}
  "linter-scss-lint": {}
  "markdown-preview":
    useGitHubStyle: true
  "markdown-writer": {}
  "merge-conflicts": {}
  minimap:
    plugins:
      bookmarks: true
      "git-diff": true
  "one-dark-ui": {}
  "one-light-ui":
    layoutMode: "Compact"
  pigments:
    sortPaletteColors: "by color"
  "rails-rspec": {}
  react: {}
  "relative-line-numbers":
    showNormalLineNumbers: false
  rspec:
    command: "bundle exec bin/rspec"
  "ruby-test":
    cucumberAllCommand: "bundle exec cucumber features"
    rspecAllCommand: "source ~/.zshrc && bundle exec bin/rspec spec"
    rspecFileCommand: "source ~/.zshrc && bundle exec bin/rspec --tty {relative_path}"
    rspecSingleCommand: "source ~/.zshrc && bundle exec bin/rspec --tty {relative_path}:{line_number}"
    shell: "/usr/local/bin/zsh"
    specFramework: "rspec"
    testAllCommand: "bundle exec rake"
  tabs: {}
  tasks: {}
  "tool-bar":
    position: "Left"
    visible: false
  "tree-view":
    hideIgnoredNames: true
  "vim-mode": {}
  "vim-surround": {}
  welcome:
    showOnStartup: false
  wordcount:
    extensions: [
      "md"
    ]
".go.source":
  editor:
    tabLength: 8
".js.jsx.source":
  editor:
    autoIndent: true
